spring.datasource.url=jdbc:h2:mem:testdb
spring.datasource.driverClassName=org.h2.Driver
spring.datasource.username=sa
spring.datasource.password=

spring.jpa.database-platform=org.hibernate.dialect.H2Dialect
spring.jpa.hibernate.ddl-auto=update

# Ativa o console web do H2 (opcional, muito ï¿½til para ver os dados direto no browser)
spring.h2.console.enabled=true
spring.h2.console.path=/h2-console

# inserir chave aqui
spring.security.oauth2.resourceserver.jwt.secret-key=

jwt.secret-key=${spring.security.oauth2.resourceserver.jwt.secret-key}
jwt.expiration=360000